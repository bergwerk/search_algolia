plugin {
    tx_searchcore {
        settings {
            connections {
                algolia {
                    applicationID = {$page.search.algolia.applicationID}
                    apiKey = {$page.search.algolia.apiKey}
                }
            }
        }
    }
}

module.tx_searchcore < plugin.tx_searchcore

[applicationContext = Development]
    plugin {
        tx_searchcore {
            settings {
                debug = 1
            }
        }
    }
[end]
